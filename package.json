{
  "name": "the-not-boring-list",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "node --watch --env-file .env server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules RUN_SKIPPED=true jest --runInBand",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "db:service:up": "docker compose up -d",
    "db:service:down": "docker compose down",
    "migrations:up": "node-pg-migrate --migrations-dir src/infra/migrations --envPath .env.development up",
    "migrations:create": "node-pg-migrate --migrations-dir src/infra/migrations create",
    "commit": "cz c"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.876.0",
    "bcryptjs": "3.0.2",
    "express": "^5.1.0",
    "multer": "2.0.2",
    "node-pg-migrate": "8.0.3",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@faker-js/faker": "9.9.0",
    "@types/jest": "^30.0.0",
    "cz": "1.8.2",
    "dotenv": "17.2.1",
    "dotenv-expand": "12.0.2",
    "jest": "^30.0.5",
    "supertest": "^7.1.4"
  },
